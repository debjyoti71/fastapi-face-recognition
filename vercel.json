{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "env": {
    "CLOUDINARY_CLOUD_NAME": "@cloud_name",
    "CLOUDINARY_API_KEY": "@api_key",
    "CLOUDINARY_API_SECRET": "@api_secret"
  }
}
